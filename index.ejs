<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Goals</title>
</head>
<body>
    <h1>Goals</h1>
    <ul>
      <% goals.forEach(function(g) { %>
        <li>
          <%= g.goal %>
            - 
          <%= g.done ? 'done' : 'not done' %>
        </li>
      <% }); %>
    </ul>
  </body>
</html>